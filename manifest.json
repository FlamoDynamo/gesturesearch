{
    "manifest_version": 3,
    "name": "Phần mềm tìm kiếm Google bằng ngôn ngữ ký hiệu",
    "version": "1.0",
    "permissions": ["activeTab", "camera"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }